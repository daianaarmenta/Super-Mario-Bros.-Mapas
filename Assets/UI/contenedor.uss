.titulo {
    align-self: center;
    width: 80%;
    border-top-width: 50px;
    border-right-width: 50px;
    border-bottom-width: 50px;
    border-left-width: 50px;
    background-color: rgba(0, 0, 0, 0.35);
    background-size: 100% 100%;
    height: 15%;
    translate: 0 70px;
    border-radius: 40px;
}

.contenedor {
    width: 100%;
}

.botones {
    background-color: rgba(55, 55, 55, 0.35);
    width: 80%;
    height: 100%;
    border-radius: 40px;
}
